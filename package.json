{
  "name": "dashboard-core",
  "version": "1.0.0",
  "description": "",
  "type": "module",
  "main": "./src/index.ts",
  "exports": {
    ".": "./src/index.ts",
    "./*": "./src/*.ts"
  },
  "scripts": {
    "build": "tsup",
    "dev": "concurrently \"npm run dev:tsup\" \"npm run dev:barrels\"",
    "start": "npm run dev",
    "dev:tsup": "tsup --watch",
    "dev:barrels": "barrelsby --directory ./src --exclude \"node_modules\" --exclude \"src/backend\" --exclude \"test.ts\"  --delete --name \"index.ts\" --watch",
    "barrels": "barrelsby --directory ./src --exclude \"node_modules\" --exclude \"src/backend\" --exclude \"test.ts\"  --delete --name \"index.ts\"",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "@mui/icons-material": "^6.1.5",
    "@mui/material": "^6.1.5",
    "@sendgrid/mail": "^8.1.4",
    "@types/node": "^22.8.7",
    "@types/react": "^18.3.12",
    "axios": "^1.7.7",
    "barrelsby": "^2.8.1",
    "concurrently": "^8.2.2",
    "firebase": "^11.0.1",
    "firebase-admin": "^12.7.0",
    "libsodium-wrappers": "^0.7.15",
    "multiformats": "^13.3.1",
    "next": "14.0.1",
    "ra-core": "^5.3.0",
    "ra-i18n-polyglot": "^5.3.0",
    "ra-language-english": "^5.3.0",
    "ra-language-french": "^5.3.0",
    "react": "^18.3.1",
    "react-admin": "^5.3.0",
    "react-admin-firebase": "^4.1.3",
    "react-cropper": "^2.3.3",
    "react-dropzone": "^14.2.10",
    "react-hook-form": "^7.53.1",
    "react-router": "^6.27.0",
    "react-router-dom": "^6.27.0",
    "tsup": "^8.3.5",
    "typescript": "^5.6.3"
  }
}
